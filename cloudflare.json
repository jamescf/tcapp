{
    "main":"public/javascripts/mytest.js",
    "name":"FlareRelic",
    "description": "A CloudFlare application uses FlareRelic",
    "version": "0.0.5",
    "account": {
        "callback_url": "http://countersback.herokuapp.com/api",
        "user_fields": ["email"]
    },
    "billing": {
        "type": "zone",
        "plans":[
            {
                "name" : "Basic",
                "price": "3.20"
            },
            {
                "name" : "Advanced",
                "price" : "6.55"
            }
        ]
    },
    "config":{
        "details":{
            "Price": "priceless",
            "Supporting": "cats"
        },
        "assets":{
            "logos" : {
                "200px": "./public/images/logo-200.png",
                "132px": "./public/images/logo-132.png"
            },
            "detail_page" : [
                "./public/images/example.png"
            ],
            "images" : [
                "./public/images/counter.png"
            ]
        },
        "interface": [
            {
                "id": "complexity",
                "type": "select",
                "name": "Complexity Level",
                "description": "Choose the graphing complexity",
                "options": [
                    {
                        "label": "Basic",
                        "value": "0"
                    },
                    {
                        "label": "Advanced",
                        "value": "1"
                    }
                ]
            }
        ]
    }
}
